{
	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "Debug Nest Framework",
			"args": ["${workspaceFolder}/src/main.ts"],
			"sourceMaps": true,
			"cwd": "${workspaceRoot}",
			"runtimeExecutable": "npm",
			"runtimeArgs": ["run", "start:debug", "--", "--inspect-brk"],
			"console": "integratedTerminal",
			"restart": true,
			"protocol": "auto",
			"autoAttachChildProcesses": true
		}
	]
}
